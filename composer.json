{
  "name": "infobiotech/php-json-cache",
  "description": "JSON-based PSR-16 cache implementation.",
  "type": "library",
  "keywords": [
    "cache",
    "psr-16",
    "json",
    "flysistem"
  ],
  "homepage": "https://github.com/infobiotech/php-json-cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Alessandro Raffa, Infobiotech S.r.l.",
      "email": "a.raffa@infobiotech.net",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/infobiotech/php-json-cache/issues"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^5.6 || ^7.0",
    "league/flysystem": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^5.2",
    "phpunit/phpunit": "^6.4",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Infobiotech\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check": "phpcs src --standard=psr2 -sp"
  }
}
